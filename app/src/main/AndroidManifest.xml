<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.clx.cpal.clxand">


    <uses-permission android:name="android.permission.INTERNET"/>

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <service android:name=".SuMaGuanV1.SocketService"/>

        <activity android:name=".LoginActivity"></activity>
        <activity
            android:name=".EasyCaptureActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CustomCaptureActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CodeActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".MainActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".SuMaGuanActivity" />
        <activity android:name=".SuMaGuanV1.SuMaGuan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SuMaGuanV1.SuMaGuanAction"></activity>
    </application>

</manifest>